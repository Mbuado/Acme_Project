{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Sid": "DynamoDBAccess",
          "Effect": "Allow",
          "Action": [
              "dynamodb:GetItem",
              "dynamodb:PutItem",
              "dynamodb:Scan",
              "dynamodb:Query"
          ],
          "Resource": "*"
      },
      {
          "Sid": "CloudWatchLogsAccess",
          "Effect": "Allow",
          "Action": [
              "logs:CreateLogGroup",
              "logs:CreateLogStream",
              "logs:PutLogEvents"
          ],
          "Resource": "*"
      }
  ]
}
